  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script>
    $(".card-toggle-btn").on("click", function () {
      if ($(this).hasClass("active") || $(".card-toggle-btn").is(":animated")) {
        return; // Prevent clicking while animations are ongoing or the button is already active
      }
  
      // Mark animation as ongoing to prevent multiple triggers
      var isAnimating = true;
  
      // Remove active class from all buttons and reset their states
      $(".card-toggle-btn").removeClass("active");
      $(this).addClass("active");
  
      // Reset the z-index and active class from previous items
      $(".card-item").find(".card-content-container").css("z-index", 0);
      $(".card-item").removeClass("active");
  
      // Ensure the clicked button's content container is visible and on top
      var that = $(this);
      $(this).siblings().css("z-index", 1);
  
      setTimeout(function () {
        // Toggle active class for the corresponding card content
        that.parent().toggleClass("active").find(".card-content-container").on("transitionend", function () {
          // Set animation flag to false when the transition is done
          isAnimating = false;
        });
      }, 10);
    });
  
    // Handle input blur events for form fields
    $("input,textarea").blur(function () {
      if ($(this).val()) {
        $(this).parent().addClass("filled");
      } else {
        $(this).parent().removeClass("filled");
      }
    });
  
    // Toggle visibility of contact form container
    $(".contact-btn").on("click", function () {
      $(".contact-form-container").toggleClass("active");
    });
  
    $(".contact-form-container input[type=submit], .contact-form-container .close-btn").on("click", function (e) {
      e.preventDefault();
      $(".contact-form-container").toggleClass("active");
    });
  </script>
  
